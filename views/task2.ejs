<!DOCTYPE html>
<html>
  <head>
    <title>Task 2</title>
    <link rel="stylesheet" href="/styles.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
  
  </head>
<body class="bg-dark">
  <nav>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav">
          <li class="nav-item">
            <a class="nav-link" href="/"><p class="display-4" style="font-size: 25px;">Task 1</p></a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/task2"><p class="display-4" style="font-size: 25px;">Task 2</p></a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/task3"><p class="display-4" style="font-size: 25px;">Task 3</p></a>
          </li>
        </ul>
      </div>
    </nav>
  <!-- <h1>Task 2 - Customer Data</h1> -->
  <table class="table table-dark">
    <thead>
      <tr>
        <th>Customer ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email Address</th>
        <th>Phone</th>
      </tr>
    </thead>
    <tbody>
      <%customers.forEach(customer =>{ %>
        <tr>
          <td><%= customer.CustomerID %></td>
          <td><%= customer.FirstName %></td>
          <td><%= customer.LastName %></td>
          <td><%= customer.EmailAddress %></td>
          <td><%= customer.Phone %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>
